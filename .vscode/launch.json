      "type": "node",
      "request": "launch",
      "env": {
      },
      "name": "Debug Unit Test",
      "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
      "args": ["--reporter=jasmine-reporter-console"],
      "outputCapture": "std"
    },
  ]
}